PROJECT_NAME = PeerNet: A P2P Library based on Zyre (CZMQ)
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/doc/
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/include/peer.h @CMAKE_CURRENT_SOURCE_DIR@/README.MD @CMAKE_CURRENT_SOURCE_DIR@/CHANGELOG.MD
PROJECT_NUMBER = "@PEER_VERSION@"
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/README.MD
EXTRACT_STATIC = YES
GENERATE_HTML = YES
HTML_OUTPUT = html
HTML_FILE_EXTENSION = .html
GENERATE_LATEX = YES
LATEX_OUTPUT = latex
USE_PDFLATEX = YES
PAPER_TYPE = letter
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
PREDEFINED             = __attribute__(x)=
PREDEFINED = _DOXYGEN_
EXTRACT_STATIC = YES